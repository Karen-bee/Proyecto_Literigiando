-- --------------------------------------------------------

--
-- Estructura de tabla para la tabla `libros`
--

CREATE TABLE `libros` (
  `id_libro` bigint NOT NULL AUTO_INCREMENT,
  `titulo` varchar(101) COLLATE utf8mb4_swedish_ci NOT NULL,
  `autor` varchar(51) COLLATE utf8mb4_swedish_ci NOT NULL,
  `tema` varchar(51) COLLATE utf8mb4_swedish_ci NOT NULL,
  `descripcion`  varchar(501) COLLATE utf8mb4_swedish_ci NOT NULL,
  `condicion_libro` varchar(51) COLLATE utf8mb4_swedish_ci NOT NULL,
  `edades` varchar(18) COLLATE utf8mb4_swedish_ci NOT NULL,
  `color` varchar(9) COLLATE utf8mb4_swedish_ci NOT NULL,
  `formato` varchar(8) COLLATE utf8mb4_swedish_ci NOT NULL,
  `link` varchar(1000) COLLATE utf8mb4_swedish_ci NOT NULL,
  `portada` varchar(76) COLLATE utf8mb4_swedish_ci NOT NULL,
  `disponible` bigint NOT NULL,
  PRIMARY KEY (id_libro)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_swedish_ci;

