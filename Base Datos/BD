-- --------------------------------------------------------

--
-- Estructura de tabla para la tabla `prestamos`
--

CREATE TABLE `prestamos` (
  `id_prestamo` bigint NOT NULL AUTO_INCREMENT,
  `id_usuario` bigint NOT NULL,
  `id_libro_1` bigint NULL,
  `id_libro_2` bigint NULL,
  `id_libro_3` bigint NULL,
  `fecha_prestamo` date NOT NULL,
  `fecha_devolucion` date NOT NULL,
  `estado` bigint NOT NULL,
  `permiso` bigint NOT NULL,
  PRIMARY KEY (id_prestamo)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_swedish_ci;

-- --------------------------------------------------------

--
-- Filtros para la tabla `blogs`
--

ALTER TABLE blogs
ADD FOREIGN KEY (acceso) REFERENCES accesos(id_acceso);

-- --------------------------------------------------------

--
-- Filtros para la tabla `eventos`
--

ALTER TABLE eventos
ADD FOREIGN KEY (acceso) REFERENCES accesos(id_acceso);

-- --------------------------------------------------------

--
-- Filtros para la tabla `noticias`
--

ALTER TABLE noticias
ADD FOREIGN KEY (acceso) REFERENCES accesos(id_acceso);

-- --------------------------------------------------------

--
-- Filtros para la tabla `dioses`
--

ALTER TABLE dioses
ADD FOREIGN KEY (id_usuario) REFERENCES usuarios(id_usuario);

-- --------------------------------------------------------

--
-- Filtros para la tabla `usuarios`
--

ALTER TABLE usuarios
ADD FOREIGN KEY (rol) REFERENCES roles(id_rol),
ADD FOREIGN KEY (acceso) REFERENCES accesos(id_acceso);

-- --------------------------------------------------------

--
-- Filtros para la tabla `libros`
--

ALTER TABLE libros
ADD FOREIGN KEY (disponible) REFERENCES disponibles(id_disponible);

-- --------------------------------------------------------

--
-- Filtros para la tabla `prestamos`
--

ALTER TABLE prestamos
ADD FOREIGN KEY (id_usuario) REFERENCES usuarios(id_usuario),
ADD FOREIGN KEY (id_libro_1) REFERENCES libros(id_libro),
ADD FOREIGN KEY (id_libro_2) REFERENCES libros(id_libro),
ADD FOREIGN KEY (id_libro_3) REFERENCES libros(id_libro),
ADD FOREIGN KEY (permiso) REFERENCES permisos(id_permiso),
ADD FOREIGN KEY (estado) REFERENCES estados(id_estado);

